# ICARUS Server Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to source control!

# ===== Network Configuration =====
SERVER_IP=192.168.X.X
SERVER_VLAN_SUBNET=192.168.X.0/24
MAIN_LAN_SUBNET=192.168.Y.0/24
PUBLIC_IP=YOUR_PUBLIC_IP_HERE

# ===== Server Ports =====
GAME_PORT=17777
QUERY_PORT=27025

# ===== Server Settings =====
SERVER_NAME=My ICARUS Server
MAX_PLAYERS=8

# ===== Security =====
JOIN_PASSWORD=
ADMIN_PASSWORD=

# ===== Optional Paths =====
# Leave blank to use default %LOCALAPPDATA%\Icarus
USER_DIR=

# ===== Steam Configuration =====
STEAM_APP_ID=2089300
